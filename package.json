{
  "name": "how",
  "version": "0.1.0",
  "description": "Ask a cli tool about other cli tools.",
  "module": "./src/index.ts",
  "type": "module",
  "scripts": {
    "how": "bun run ./src/index.ts --",
    "compile": "bun build ./src/index.ts --compile --minify --sourcemap --outfile how && chmod +x ./how",
    "replace-bin": "sudo mv ./how /usr/local/bin/how"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
